<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Accounts.AccountsController">
   <children>
      <Button fx:id="backButton" layoutX="14.0" layoutY="21.0" mnemonicParsing="false" onAction="#goBack" prefHeight="27.0" prefWidth="101.0" text="Back" />
      <Button fx:id="logoutButton" layoutX="485.0" layoutY="21.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="101.0" text="Log Out" />
      <Label layoutX="178.0" layoutY="35.0" text="New Account Creation">
         <font>
            <Font size="26.0" />
         </font></Label>
      <TextField fx:id="newUserIDField" layoutX="220.0" layoutY="167.0" promptText="UserID Here" />
      <TextField fx:id="passwordField" layoutX="220.0" layoutY="230.0" promptText="Password Here" />
      <ComboBox fx:id="accountTypeComboBox" layoutX="221.0" layoutY="283.0" prefHeight="27.0" prefWidth="161.0" promptText="Account Type" />
      <Label layoutX="220.0" layoutY="150.0" prefHeight="17.0" prefWidth="76.0" text="New User ID" />
      <Label layoutX="220.0" layoutY="211.0" prefHeight="17.0" prefWidth="101.0" text="New Password" />
      <Button fx:id="submitButton" layoutX="236.0" layoutY="333.0" mnemonicParsing="false" onAction="#newAccount" prefHeight="27.0" prefWidth="121.0" text="Submit" />
      <Label fx:id="errorLabel" layoutX="82.0" layoutY="66.0" prefHeight="51.0" prefWidth="440.0" textAlignment="CENTER" textFill="RED">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Label layoutX="220.0" layoutY="266.0" prefHeight="17.0" prefWidth="131.0" text="Select Account Type" />
   </children>
</AnchorPane>
