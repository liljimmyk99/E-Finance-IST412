<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Decision.DecisionController">
   <children>
      <Button fx:id="backButton" layoutX="26.0" layoutY="65.0" mnemonicParsing="false" onAction="#goBack" prefHeight="27.0" prefWidth="181.0" text="Back" />
      <Button fx:id="logoutButton" layoutX="375.0" layoutY="65.0" mnemonicParsing="false" onAction="#logout" prefHeight="27.0" prefWidth="181.0" text="Log Out" />
      <Label layoutX="191.0" layoutY="14.0" text="Approve/Deny Applications">
         <font>
            <Font size="18.0" />
         </font></Label>
      <ComboBox fx:id="applicationComboBox" layoutX="150.0" layoutY="175.0" prefWidth="150.0" />
      <Label layoutX="60.0" layoutY="179.0" text="Applications:" />
      <Button fx:id="approveButton" layoutX="60.0" layoutY="348.0" mnemonicParsing="false" onAction="#approveApplication" prefHeight="25.0" prefWidth="181.0" text="Approve" />
      <Button fx:id="denyButton" layoutX="320.0" layoutY="348.0" mnemonicParsing="false" onAction="#denyApplication" prefHeight="25.0" prefWidth="181.0" text="Deny" />
   </children>
</AnchorPane>
